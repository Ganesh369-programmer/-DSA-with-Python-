   "source": [
    "def find_missing_number(nums):\n",
    "    m = max(nums) \n",
    "    s = min(nums)\n",
    "\n",
    "    l = []\n",
    "\n",
    "    for i in range(s , m+1):\n",
    "        l.append(i)\n",
    "    \n",
    "    unique = ''.join(str(item) for item in l if item not in nums )\n",
    "    \n",
    "    return unique\n",
    "\n",
    "nums = [3, 0, 1]\n",
    "find_missing_number(nums)"
   ]
