    "def is_palindrome(lst):\n",
    "   \n",
    "    reversed_list = lst[::-1]\n",
    "    print(reversed_list)\n",
    "    if lst == reversed_list:\n",
    "        return True\n",
    "    else:\n",
    "        return False\n",
    "    \n",
    "\n",
    "lst = [7, 8, 9, 8, 7]\n",
    "is_palindrome(lst)"
   ]
  }
